@model IEnumerable<Patient>
@if(Model.Count()>0){
<h6 class="display-6">Applications</h6>
<table class="table table-hover table-striped">
    <thead>
       <tr>
        <th>Email</th>
        <th>Treatment</th>
        <th>Full Name</th>
        <th>Date</th>
       </tr>
    </thead>
    <tbody>
        @foreach(Patient item in Model){
            <tr>
                <td>@item.Email</td>
                <td>@item.FullName</td>
            
            </tr>
        }
    </tbody>
</table>

}
else{<div class="alert alert-info">
        There is no treatment information.Please click here to <a class="alert-link" asp-action="/">
        add.
        </a>
    </div>
}