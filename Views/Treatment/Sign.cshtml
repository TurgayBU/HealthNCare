@{
    Layout = "_Layout";
}
@model Patient
<div class="display-5" style="color: white;">Sign In</div>
<form asp-action="Sign" method="post">
<div class="form-group my-2">
    <label>Email</label>
    <input class="form-control" asp-for="SEmail" >
</div>
<div class="form-group my-2">
    <label>Password</label>
    <input class="form-control" asp-for="SPassword">
</div>
<div>
   <a class="btn btn-primary" asp-controller="Home" asp-action="Index">Sign</a>
</div>
</form>